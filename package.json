    "raw-loader": "^0.5.1",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "bpmn-js": "^8.3.1",
    "diagram-js": "^7.2.3",
    "diagram-js-origin": "^1.3.2",
    "jquery": "^3.6.0"
  }
}
